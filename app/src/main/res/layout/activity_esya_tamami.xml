<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_esya_tamami"
    android:layout_width="match_parent"
    android:background="@drawable/background"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.cengiz.cengo.Renkler">


    <ImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/kasik"
        android:id="@+id/kasik"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:onClick="oku"/>

    <ImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/tabak"
        android:id="@+id/tabak"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="oku"/>

    <ImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/sandalye"
        android:id="@+id/sandalye"
        android:layout_alignTop="@+id/masa"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="oku"/>

    <ImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/yatak"
        android:id="@+id/yatak"
        android:layout_alignTop="@+id/dolap"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:onClick="oku"/>

    <TextView
        android:text="Çatal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/catal"
        android:layout_marginTop="16dp"
        android:id="@+id/text_catal"
        android:layout_alignLeft="@+id/text_bardak"
        android:layout_alignStart="@+id/text_bardak" />

    <ImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/yastik"
        android:id="@+id/yastik"
        android:layout_marginBottom="57dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/dolap"
        android:layout_alignStart="@+id/dolap"
        android:onClick="oku"/>

    <TextView
        android:text="Kaşık"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/text_catal"
        android:layout_alignRight="@+id/kasik"
        android:layout_alignEnd="@+id/kasik"
        android:id="@+id/text_kasik"
        android:layout_alignLeft="@+id/text_masa"
        android:layout_alignStart="@+id/text_masa" />

    <TextView
        android:text="Bardak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/masa"
        android:layout_marginTop="14dp"
        android:id="@+id/text_bardak"
        android:layout_alignRight="@+id/bardak"
        android:layout_alignEnd="@+id/bardak" />

    <TextView
        android:text="Sandalye"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_sandalye"
        android:layout_alignBaseline="@+id/text_masa"
        android:layout_alignBottom="@+id/text_masa"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:text="Tabak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_tabak"
        android:layout_alignBaseline="@+id/text_kasik"
        android:layout_alignBottom="@+id/text_kasik"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/text_sandalye"
        android:layout_alignStart="@+id/text_sandalye" />

    <TextView
        android:text="Masa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_masa"
        android:layout_alignBaseline="@+id/text_bardak"
        android:layout_alignBottom="@+id/text_bardak"
        android:layout_alignLeft="@+id/text_dolap"
        android:layout_alignStart="@+id/text_dolap" />

    <ImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/catal"
        android:id="@+id/catal"
        android:onClick="oku"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/masa"
        android:id="@+id/masa"
        android:onClick="oku"
        android:layout_alignTop="@+id/bardak"
        android:layout_alignRight="@+id/text_kasik"
        android:layout_alignEnd="@+id/text_kasik" />

    <ImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/bardak"
        android:id="@+id/bardak"
        android:layout_marginTop="24dp"
        android:onClick="oku"
        android:layout_below="@+id/text_catal"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Yastık"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="33dp"
        android:id="@+id/text_yastik"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/yastik"
        android:layout_alignEnd="@+id/yastik"
        android:layout_alignLeft="@+id/text_dolap"
        android:layout_alignStart="@+id/text_dolap" />

    <TextView
        android:text="Koltuk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_koltuk"
        android:layout_above="@+id/yastik"
        android:layout_alignRight="@+id/text_catal"
        android:layout_alignEnd="@+id/text_catal"
        android:layout_marginBottom="18dp" />

    <ImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/koltuk"
        android:id="@+id/koltuk"
        android:onClick="oku"
        android:layout_above="@+id/text_koltuk"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="11dp" />

    <ImageView
        android:layout_width="65dp"
        android:layout_height="65dp"
        app:srcCompat="@drawable/dolap"
        android:id="@+id/dolap"
        android:onClick="oku"
        android:layout_alignTop="@+id/koltuk"
        android:layout_alignLeft="@+id/masa"
        android:layout_alignStart="@+id/masa" />

    <TextView
        android:text="Dolap"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_dolap"
        android:layout_alignBaseline="@+id/text_koltuk"
        android:layout_alignBottom="@+id/text_koltuk"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="Yatak"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/text_yatak"
        android:layout_alignBaseline="@+id/text_dolap"
        android:layout_alignBottom="@+id/text_dolap"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignLeft="@+id/text_sandalye"
        android:layout_alignStart="@+id/text_sandalye" />

</RelativeLayout>
